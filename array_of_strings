public class Main
{
	public static void main(String[] args) {
	  
	   String[] words = new String[4];
	   words[0]="govind";
	   words[1]="amal";
	   words[2]="Suriya";
	   System.out.println(words [3]);
	  
	   String  []fruits={"apple", "orange", "banana"};
	   for(int i=0;i<fruits.length;i++)
	   {
	         System.out.println(fruits[i]);
	   }
	   
	}
}
