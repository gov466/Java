class robot{                    //class named robot
    public void speak(String text)        //method with paramter string
    {
       System.out.println(text);      //printing the text
    } 
    public void jump(int height) //another method
    {
        System.out.println("Jumping: " +  height);  //printing statement
    }
    public void move(String direction, double distance)     //another method having two parameters
    {
        System.out.println("Moving "+distance + " Mtrs in distance "+direction);
    }
}
public class HelloWorld{

     public static void main(String []args){
         robot san = new robot();             //class variable
         san.speak("hi i am govind");   //passing parameter to class
         san.jump(7);           //passing parameter to method jump
         san.move("West", 7.2); // passing paramter to method move
        
     }
}
